{
  "name": "mhc-control-panel",
  "version": "1.0.0",
  "description": "Production-grade streaming intelligence and memory system for Chaturbate",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch server/src/index.ts",
    "build": "tsc -p server/tsconfig.json",
    "start": "node dist/index.js",
    "worker": "node dist/worker.js",
    "migrate": "node dist/db/migrate.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint server/src --ext .ts",
    "format": "prettier --write \"server/src/**/*.ts\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "axios": "^1.6.5",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/pg": "^8.10.9",
    "@types/node": "^20.10.6",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/parser": "^6.18.0",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Hudson Cage",
  "license": "UNLICENSED"
}
